                "type": "string"
            },
            {
                "name": "commandParameters",
                "type": "object",
                "optional": true
            }
        ]},
        "insertImage": {"parameters": [{
            "name": "source",
            "type": "string"
        }]},
        "getHTMLData": {
            "returns": "string",
            "parameters": [{
