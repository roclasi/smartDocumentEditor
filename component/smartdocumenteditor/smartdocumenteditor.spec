            "parameters": [
                {
                    "name": "oldValue",
                    "type": "${dataproviderType}"
                },
                {
                    "name": "newValue",
                    "type": "${dataproviderType}"
                },
                {
                    "name": "event",
                    "type": "JSEvent"
                }
            ],
            "code": "return true"
        },
        "onFocusGainedMethodID" : {
